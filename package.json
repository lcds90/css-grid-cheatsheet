{
  "name": "css-flexbox-cheatsheet",
  "displayName": "CSS Cheatsheet",
  "description": "Open a CSS cheatsheet directly in VS Code.",
  "version": "3.3.0",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "license": "MIT",
  "icon": "images/icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "css.cheatsheet",
        "key": "Ctrl+K K",
        "mac": "Cmd+K K"
      }
    ],
    "commands": [
      {
        "command": "css.cheatsheet",
        "title": "Open CSS Cheatsheet"
      }
    ]
  },
  "activationEvents": [
    "onCommand:css.cheatsheet",
    "onLanguage:css",
    "onLanguage:less",
    "onLanguage:sass",
    "onLanguage:scss"
  ],
  "publisher": "dzhavat",
  "repository": {
    "type": "git",
    "url": "https://github.com/dzhavat/css-flexbox-cheatsheet"
  },
  "vsce": {
    "githubBranch": "main"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.19.11",
    "@types/vscode": "1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vscode/webview-ui-toolkit": "^0.8.4",
    "eslint": "^8.1.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "vsce": "^2.2.0",
    "vscode-test": "^1.6.1"
  }
}
